<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Test Suite">
    <test name="Booking Service Tests">
        <groups>
            <run>
                <include name="Regression"/>
                <exclude name="Quarantine"/>
            </run>
        </groups>
        <classes>
            <class name="hook.BookingHook"/>
            <class name="org.testing.BookingService.PostAuthTests"/>
            <class name="org.testing.BookingService.GetBookingTests"/>
        </classes>
    </test>
</suite>
